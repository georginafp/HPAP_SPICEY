	keywords = {Animals, Humans, Algorithms, Gene Expression Profiling, Genomics, Software, Computational Biology, Drosophila melanogaster, Mice, Models, Statistical, Pattern Recognition, Automated, Programming Languages, Reproducibility of Results, RNA-Seq, Transcriptome},
	pages = {e1007664},
	file = {Full Text:/home/georginafuentes/Zotero/storage/CM7E7A7J/Love et al. - 2020 - Tximeta Reference sequence checksums for provenan.pdf:application/pdf},
